      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${customerBusinessLogic.getCustomer(customerId).approved}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:endEvent id="EndEvent_2" name="Customer is not approved">
      <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_6" name="" sourceRef="ExclusiveGateway_1" targetRef="EndEvent_2">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${not customerBusinessLogic.getCustomer(customerId).approved}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
